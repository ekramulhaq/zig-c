int sieve[26];

int main() {
    for (int i = 0; i <= 25; ++i) {
        sieve[i] = 1;
    }
    sieve[0] = 0;
    sieve[1] = 0;
    
    for (int p = 2; p * p <= 25; ++p) {
        if (sieve[p] == 1) {
            for (int i = p * p; i <= 25; i += p) {
                sieve[i] = 0;
            }
        }
    }
    
    int sum = 0;
    for (int i = 0; i <= 25; ++i) {
        if (sieve[i] == 1) {
            sum += i;
        }
    }
    return sum; // 100
}
