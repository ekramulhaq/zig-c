char global_c = 65; // 'A'

char to_upper(char c) {
    if (c >= 97) {
        if (c <= 122) {
            return c - 32;
        }
    }
    return c;
}

int main() {
    char c = 98; // 'b'
    char upper_c = to_upper(c); // 'B' (66)

    char arr[3];
    arr[0] = 72; // 'H'
    arr[1] = 105; // 'i'
    arr[2] = 0;

    if (global_c == 65) {
        if (upper_c == 66) {
            if (arr[0] == 72) {
                if (arr[1] == 105) {
                    return 42;
                }
            }
        }
    }
    return 0;
}
