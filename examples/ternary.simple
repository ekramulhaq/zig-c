int main() {
    int a = 10;
    int b = 20;
    
    int max = (a > b) ? a : b; // max = 20
    int min = (a < b) ? a : b; // min = 10
    
    // Nested ternary
    int res = (a == 10) ? ((b == 20) ? 42 : 0) : 0;
    
    // Complex expression in ternary
    int complex = (1 + 1 == 2) ? (10 * 4 + 2) : 0; // 42
    
    if (max == 20 && min == 10 && res == 42 && complex == 42) {
        return 42;
    }
    return 0;
}
