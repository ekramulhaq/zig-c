int g1 = 10, g2 = 20;

int main() {
    int a = 1, b, c = 3;
    b = 2;
    
    int *p1, *p2;
    p1 = &a;
    p2 = &c;
    
    *p1 = 10;
    *p2 = 30;
    
    // a=10, b=2, c=30 -> sum = 42
    int sum = a + b + c;
    
    // g1=10, g2=20 -> g_sum = 30
    int g_sum = g1 + g2;
    
    if (sum == 42 && g_sum == 30) {
        return 42;
    }
    return sum;
}
