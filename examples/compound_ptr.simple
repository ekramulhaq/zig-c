struct S {
    int val;
};

int main() {
    int x = 10;
    int *p = &x;
    *p += 5; // x = 15
    *p *= 2; // x = 30
    *p -= 10; // x = 20
    *p /= 2; // x = 10
    *p %= 3; // x = 1

    int arr[5];
    arr[0] = 10;
    arr[0] += 5; // arr[0] = 15

    struct S s;
    s.val = 10;
    s.val *= 4; // s.val = 40

    if (x == 1 && arr[0] == 15 && s.val == 40) {
        return 42;
    }
    return 0;
}
